S_0x64ff85e50480 .scope module, "mux4_1_tb" "mux4_1_tb" 2 3;
 .timescale 0 0;
v0x64ff85e79380_0 .var "data_in", 3 0;
v0x64ff85e79490_0 .var "sel_in", 1 0;
v0x64ff85e79560_0 .net "y", 0 0, v0x64ff85e78cd0_0;  1 drivers
S_0x64ff85e5ef00 .scope module, "dut" "mux4_1" 2 12, 3 2 0, S_0x64ff85e50480;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "data_in";
    .port_info 1 /INPUT 2 "sel_in";
    .port_info 2 /OUTPUT 1 "y";
v0x64ff85e267b0_0 .net "data_in", 3 0, v0x64ff85e79380_0;  1 drivers
v0x64ff85e26ba0_0 .net "sel_in", 1 0, v0x64ff85e79490_0;  1 drivers
v0x64ff85e78cd0_0 .var "y", 0 0;
E_0x64ff85e258a0 .event edge, v0x64ff85e26ba0_0, v0x64ff85e267b0_0;
S_0x64ff85e78df0 .scope task, "initial_c" "initial_c" 2 20, 2 20 0, S_0x64ff85e50480;
 .timescale 0 0;
TD_mux4_1_tb.initial_c ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x64ff85e79380_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x64ff85e79490_0, 0, 2;
    %delay 10, 0;
    %vpi_call 2 25 "$display", $time, "inputs:--- data_in=%b , sel_in=%b , output:--- y=%b ", v0x64ff85e79380_0, v0x64ff85e79490_0, v0x64ff85e79560_0 {0 0 0};
    %end;
S_0x64ff85e78fd0 .scope task, "stimuli" "stimuli" 2 30, 2 30 0, S_0x64ff85e50480;
 .timescale 0 0;
v0x64ff85e791e0_0 .var "d", 3 0;
v0x64ff85e792a0_0 .var "sel", 1 0;
TD_mux4_1_tb.stimuli ;
    %load/vec4 v0x64ff85e791e0_0;
    %store/vec4 v0x64ff85e79380_0, 0, 4;
    %load/vec4 v0x64ff85e792a0_0;
    %store/vec4 v0x64ff85e79490_0, 0, 2;
    %delay 10, 0;
    %vpi_call 2 35 "$display", $time, "inputs:-- data_in=%b , sel_in=%b , outputs:--- y=%b", v0x64ff85e79380_0, v0x64ff85e79490_0, v0x64ff85e79560_0 {0 0 0};
    %end;
    .scope S_0x64ff85e5ef00;
T_2 ;
    %wait E_0x64ff85e258a0;
    %load/vec4 v0x64ff85e26ba0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_2.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_2.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_2.3, 6;
    %pushi/vec4 0, 1, 1;
    %store/vec4 v0x64ff85e78cd0_0, 0, 1;
    %jmp T_2.5;
T_2.0 ;
    %load/vec4 v0x64ff85e267b0_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x64ff85e78cd0_0, 0, 1;
    %jmp T_2.5;
T_2.1 ;
    %load/vec4 v0x64ff85e267b0_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x64ff85e78cd0_0, 0, 1;
    %jmp T_2.5;
T_2.2 ;
    %load/vec4 v0x64ff85e267b0_0;
    %parti/s 1, 2, 3;
    %store/vec4 v0x64ff85e78cd0_0, 0, 1;
    %jmp T_2.5;
T_2.3 ;
    %load/vec4 v0x64ff85e267b0_0;
    %parti/s 1, 3, 3;
    %store/vec4 v0x64ff85e78cd0_0, 0, 1;
    %jmp T_2.5;
T_2.5 ;
    %pop/vec4 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x64ff85e50480;
T_3 ;
    %vpi_call 2 44 "$display", "-----4:1 MUX using Behavioral abstraction-----" {0 0 0};
    %vpi_call 2 45 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 46 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x64ff85e50480 {0 0 0};
    %fork TD_mux4_1_tb.initial_c, S_0x64ff85e78df0;
    %join;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x64ff85e791e0_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x64ff85e792a0_0, 0, 2;
    %fork TD_mux4_1_tb.stimuli, S_0x64ff85e78fd0;
    %join;
    %delay 5, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x64ff85e791e0_0, 0, 4;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x64ff85e792a0_0, 0, 2;
    %fork TD_mux4_1_tb.stimuli, S_0x64ff85e78fd0;
    %join;
    %delay 5, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x64ff85e791e0_0, 0, 4;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x64ff85e792a0_0, 0, 2;
    %fork TD_mux4_1_tb.stimuli, S_0x64ff85e78fd0;
    %join;
    %delay 5, 0;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x64ff85e791e0_0, 0, 4;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x64ff85e792a0_0, 0, 2;
    %fork TD_mux4_1_tb.stimuli, S_0x64ff85e78fd0;
    %join;
    %delay 5, 0;
    %delay 5, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x64ff85e791e0_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x64ff85e792a0_0, 0, 2;
    %fork TD_mux4_1_tb.stimuli, S_0x64ff85e78fd0;
